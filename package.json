{
  "name": "webrtc-detection",
  "version": "1.0.0",
  "description": "Real-time WebRTC Multi-Object Detection",
  "main": "backend/src/server.js",
  "scripts": {
    "start": "node backend/src/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/src/server.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "bench": "./bench/run_bench.sh"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "wrtc": "^0.4.7",
    "onnxruntime-node": "^1.16.3",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "cors": "^2.8.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["webrtc", "object-detection", "real-time", "computer-vision"],
  "author": "Claude",
  "license": "MIT"
}